{
  "name": "@makerx/caddi-poller",
  "version": "1.0.0",
  "description": "Caddi Poller",
  "main": "index.js",
  "author": "MakerX",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --watch .env -r dotenv/config ./index.ts",
